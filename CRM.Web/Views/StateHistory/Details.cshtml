@model CRM.Web.Models.StateHistoryViewModel
@{
    ViewBag.Title = Resources.StateHistory.DetailsTitle;
    Layout = "~/Views/Shared/_LayoutCMS.cshtml";
}
<main>
    <section class="section">
        <div class="container-fluid">

            <header class="section-header justify-content-between flex-wrap" id="headerIndex">
                <div class="d-flex">
                    <button class="ml-2 sidebar-toggle d-lg-none"><i class="fa fa-bars"></i></button>
                    <h2 class="section-title">@Resources.UserProfile.Title</h2>
                </div>
            </header>

            <table class="c-table mb-4">
                <tr>
                    <td>@Resources.StateHistory.CaseName</td>
                    <td>@Model.CaseName</td>
                </tr>
                <tr>
                    <td>@Resources.StateHistory.PreviousState</td>
                    <td>@Model.PreviousStateName</td>
                </tr>
                <tr>
                    <td>@Resources.StateHistory.CurrentStateName</td>
                    <td>@Model.CurrentStateName</td>
                </tr>
                <tr>
                    <td>@Resources.StateHistory.TxtCreatedBy</td>
                    <td>@Model.CreatedBy</td>
                </tr>
                <tr>
                    <td>@Resources.StateHistory.TxtCreatedOn</td>
                    <td>@Model.CreatedOn</td>
                </tr>
            </table>

        </div>
    </section>

</main>